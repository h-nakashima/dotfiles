#!/usr/bin/env bash

# Install xcode.
xcode-select --install

# Install Homebrew.
/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"

# Change macOS settings.
/bin/bash .macos

# Install apps from brew.sh file.
/bin/bash brew.sh

# Install apps from AppStore.
/bin/bash .macos_apps